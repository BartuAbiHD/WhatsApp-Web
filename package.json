{
  "name": "WhatsApp Web",
  "version": "1.0.0",
  "description": "WhatsApp Web",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build-win": "electron-builder --win",
    "dev": "electron . --dev",
    "package-win": "electron-packager . WhatsApp_Web --platform=win32 --arch=x64 --out=dist --overwrite --icon=assets/icon.ico"
  },
  "keywords": [
    "whatsapp",
    "whatsapp web",
    "electron",
    "webview"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2"
  },
  "build": {
    "appId": "com.whatsapp.web",
    "productName": "WhatsApp Web",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    }
  },
  "dependencies": {}
}
